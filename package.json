{
  "name": "divi-child",
  "version": "3.0.0",
  "description": "A Divi Child Theme with GDPR and page speed optimizations",
  "author": "Mirko Schubert <info@mirkoschubert.de>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "lint": "eslint admin-app/src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:pot": "pnpm exec makepot . languages --domain theme --slug divi-child",
    "build:json": "pnpm exec makejson languages --scriptName ../admin-app/build/admin-app.js",
    "build:translations": "pnpm run build:pot && pnpm run build:json"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.24.0",
    "@wordpress/components": "^29.10.0",
    "@wordpress/element": "^6.24.0",
    "@wordpress/i18n": "^5.24.0",
    "@wordpress/icons": "^10.24.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@wp-blocks/make-pot": "^1.6.3",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "minimatch": "^9.0.5",
    "style-loader": "^4.0.0",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "license": "GPL-3.0"
}
